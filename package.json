  "name": "hello-express",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "busboy": "^0.3.1",
    "cheerio": "^1.0.0-rc.3",
    "cross-env": "^5.2.1",
    "crypto-js": "^3.1.9-1",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "fast-csv": "^3.4.0",
    "gm": "^1.23.1",
    "googleapis": "^39.2.0",
    "js-base64": "^2.5.1",
    "mailparser": "^2.7.1",
    "merge": "^1.2.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.0",
    "multer": "^1.4.1",
    "mysql2": "^1.7.0",
    "nodemailer": "^6.3.0",
    "nodemon": "^1.19.1",
    "open": "^7.0.0",
    "react-responsive-carousel": "^3.1.50",
    "sha1": "^1.1.1",
    "sse-pubsub": "^1.2.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
